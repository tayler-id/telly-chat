{
  "servers": [
    {
      "name": "filesystem",
      "url": "stdio://npx/@modelcontextprotocol/server-filesystem",
      "description": "Access local filesystem - read, write, list files",
      "enabled": false,
      "capabilities": ["read", "write", "list"],
      "environment": {
        "ALLOWED_DIRECTORIES": "/Users/tramsay/Desktop/_ORGANIZED/01_Development/telly-chat"
      }
    },
    {
      "name": "github",
      "url": "stdio://npx/@modelcontextprotocol/server-github",
      "description": "Access GitHub repositories, issues, and pull requests",
      "enabled": false,
      "capabilities": ["repos", "issues", "pulls"],
      "environment": {
        "GITHUB_TOKEN": ""
      }
    },
    {
      "name": "web-search",
      "url": "stdio://npx/@modelcontextprotocol/server-websearch",
      "description": "Search the web for current information",
      "enabled": false,
      "capabilities": ["search"],
      "environment": {
        "SEARCH_API_KEY": ""
      }
    },
    {
      "name": "memory",
      "url": "stdio://npx/@modelcontextprotocol/server-memory",
      "description": "Persistent memory storage across sessions",
      "enabled": false,
      "capabilities": ["store", "retrieve", "search"]
    },
    {
      "name": "postgres",
      "url": "stdio://npx/@modelcontextprotocol/server-postgres",
      "description": "Connect to PostgreSQL databases",
      "enabled": false,
      "capabilities": ["query", "schema"],
      "environment": {
        "DATABASE_URL": ""
      }
    }
  ]
}